<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <configSections>
    <section name="system.applicationHost"
             overrideModeDefault="Allow"
             type="Microsoft.Web.Administration.ConfigurationSection, Microsoft.Web.Administration, Version=7.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
             allowDefinition="MachineToApplication"/>
  </configSections>

  <system.applicationHost>
    <sites>
      <site name="WebsiteGym.Web" id="1">
        <application path="/">
          <virtualDirectory path="/" physicalPath="C:\site" />
        </application>
        <bindings>
          <binding protocol="http" bindingInformation="*:44336:localhost" />
        </bindings>
      </site>
    </sites>
    <applicationPools>
      <add name="Clr4IntegratedAppPool" managedRuntimeVersion="v4.0" />
    </applicationPools>
  </system.applicationHost>
</configuration>
